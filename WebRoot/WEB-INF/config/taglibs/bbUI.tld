  <tag>
    <name>summaryViewLink</name>
    <tag-class>blackboard.servlet.tag.summary.SummaryViewLinkTag</tag-class>
    <body-content>JSP</body-content>
    <description>This tag generates a link to access a 360 view</description>
    <attribute>
      <name>item</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The LineItem for the target object you want to link to.  Either this or
        entityId must be set.
      </description>
    </attribute>
    <attribute>
      <name>entityId</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The Id of the entity to link to.  Either this or item must be set.
      </description>
    </attribute>
    <attribute>
      <name>entitlement</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The entitlement to check to see if you can link to the 360 view.  If an
        entitlement is not given, this tag will not do a check and will defer
        to the surrounding page to do the security check.
      </description>
    </attribute>
    <attribute>
      <name>contextId</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The Id of the context the linked entity is in, for entitlements checks.  If
        entityId AND item are not set, this context is used as the entity Id.
      </description>
    </attribute>
    <attribute>
      <name>public</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Marks whether the entity is public or not.  Defaults to false.
      </description>
    </attribute>
    <attribute>
      <name>icon</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        URL for the icon to show.  Defaults to standard 360 View icon.
      </description>
    </attribute>
    <attribute>
      <name>styleClass</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        CSS style class to use for the link
      </description>
    </attribute>
    <attribute>
      <name>href</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        HREF for the link that is generated.  Overrides the built in logic
        for generating based on the entity data type.
      </description>
    </attribute>
    <attribute>
      <name>altText</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Alternative text to display for the link that is generated.
      </description>
    </attribute>
    <attribute>
      <name>var</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Variable to use to store the SummaryViewLink object
      </description>
    </attribute>
    <attribute>
      <name>scope</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Scope to store the SummaryViewLink object in.  Defaults to page scope.
      </description>
    </attribute>
  </tag>

