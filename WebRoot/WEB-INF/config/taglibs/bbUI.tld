<?xml version="1.0" encoding="ISO-8859-1"?>
<?xml-stylesheet type="text/xsl" href="bbNG.xsl"?>

<taglib xmlns="http://java.sun.com/xml/ns/j2ee"
        xmlns:bb="http://www.blackboard.com/tld-public-api"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd"
        version="2.0">

  <tlib-version>1.0</tlib-version>
  <short-name>bbUI</short-name>
  <uri>/bbUI</uri>
  <description>
    The Blackboard User Interface tag library encapsulates Blackboard user interface
    elements to allow developers easy access to the Blackboard look and feel
  </description>

  <tag bb:public="true">
    <name>copyright</name>
    <tag-class>blackboard.servlet.tags.CopyrightTag</tag-class>
    <body-content>empty</body-content>
    <description>Standard HTML snippet for Blackboard copyright notice</description>
    <attribute>
      <name>showFooterImage</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        This attribute is added in case the footer needs to be displayed without the image
      </description>
    </attribute>
  </tag>

  <tag bb:public="true">
    <name>inlineScript</name>
    <tag-class>blackboard.servlet.tags.InlineScriptTag</tag-class>
    <body-content>JSP</body-content>
    <description>Inserts inline script into HTML.</description>
    <attribute>
      <name>scriptLanguage</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Assumed to be JavaScript, if no input is provided</description>
    </attribute>
    <attribute>
      <name>scriptRef1</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Reference to existing script.</description>
    </attribute>
    <attribute>
      <name>scriptRef2</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Reference to another existing script.</description>
    </attribute>
    <attribute>
      <name>scriptRef3</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Reference to another existing script.</description>
    </attribute>
    <attribute>
      <name>scriptRef4</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Reference to another existing script.</description>
    </attribute>
  </tag>

  <tag bb:public="true">
    <name>spacer</name>
    <tag-class>blackboard.servlet.tags.SpacerTag</tag-class>
    <body-content>empty</body-content>
    <description>
      Creates an invisible area on the page with the specified height and width
    </description>
    <attribute>
      <name>height</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Vertical size of the spacer in pixels [1]</description>
    </attribute>
    <attribute>
      <name>width</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Horizontal size of the spacer in pixels [1]</description>
    </attribute>
    <attribute>
      <name>align</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description><![CDATA[<i>deprecated</i>]]></description>
    </attribute>
  </tag>

  <tag bb:public="true">
    <name>pageHeader</name>
    <tag-class>blackboard.servlet.tags.PageHeaderTag</tag-class>
    <body-content>JSP</body-content>
    <description>
      Encapsulates the top of every bb page (from Caliper on).  Includes BreadcrumbBar, InlineReceipt, and TitleBar.  Must be used inside either
      <![CDATA[<bbClp:caliperPage navItem="thisPage"> or <bbData:context navItem="thisPage">]]>.  The navitem is important, as it is used
      to seed the breadcrumbBar and titleBar tags.  If the navItem is not properly set by one of these two tags, the pageHeader tag will
      assume no breadcrumb or titlebar (in which case, the tag doesn't do a whole lot).
    </description>
    <attribute>
      <name>pageTitle</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Only for use in cases where the navItem label is different from the desired page title.  Use sparingly.</description>
    </attribute>
    <attribute>
      <name>showBreadcrumbBar</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <type>boolean</type>
      <description>True by default.  Toggle to hide the breadcrumbBar tag.</description>
    </attribute>
    <attribute>
      <name>showTitleBar</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <type>boolean</type>
      <description>True by default.  Toggle to hide the titleBar tag.</description>
    </attribute>
    <attribute>
      <name>showInlineReceipt</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <type>boolean</type>
      <description>True by default.  Toggle to hide the inlineReceipt tag.</description>
    </attribute>
    <attribute>
      <name>iconUrl</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Manually set the icon URL for the page title. The default value for this is the same as the default value for icon url for the titlebar tag.
      </description>
    </attribute>
    <attribute>
      <name>isSequenceMapStyle</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Whether the titlebar is being used in a wizard or sequence map.
        Intended for internal use only.
      </description>
    </attribute>
  </tag>

  <tag bb:public="true">
    <name>devDocTemplate</name>
    <tag-class>blackboard.servlet.tags.DocumentTemplateTag</tag-class>
    <body-content>JSP</body-content>
    <description>
      Combines the docTemplateHead and docTemplateBody tag (for use when you don't need to
      add items to the HEAD of the HTML doc)
    </description>
    <attribute>
      <name>title</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The page title to be displayed.</description>
    </attribute>
  </tag>

  <tag bb:public="true">
    <name>docTemplate</name>
    <tag-class>blackboard.servlet.tags.ProductionDocTemplateTag</tag-class>
    <body-content>JSP</body-content>
    <description>
      Combines the docTemplateHead and docTemplateBody tag (for use when you don't need to
      add items to the HEAD of the HTML doc
    </description>
    <attribute>
      <name>title</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The page title to be displayed.</description>
    </attribute>
    <attribute>
      <name>onLoad</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Javascript to run when the page is loaded</description>
    </attribute>
    <attribute>
      <name>onUnload</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Javascript to run when the page is unloaded</description>
    </attribute>
    <attribute>
      <name>onSubmit</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Javascript to run when the page is submitted</description>
    </attribute>
  </tag>

  <tag bb:public="true">
    <name>docTemplateHead</name>
    <tag-class>blackboard.servlet.tags.ProductionDocTemplateHeadTag</tag-class>
    <body-content>JSP</body-content>
    <description>
      Sets head defaults, such as including the base css sheet and validateForm.js links.
      Use this tag if you wish to add more style sheets or javascript to the head of your
      HTML doc
    </description>
    <attribute>
      <name>title</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The page title to be displayed.</description>
    </attribute>
    <attribute>
      <name>outputJsCss</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Whether to output the default js/css to the page. Default true.</description>
    </attribute>
  </tag>

  <tag bb:public="true">
    <name>docTemplateBody</name>
    <tag-class>blackboard.servlet.tags.ProductionDocTemplateBodyTag</tag-class>
    <body-content>JSP</body-content>
    <description>
      Sets the default body tag settings for the HTML page (such as attaching the default
      style class to the body tag)
    </description>
    <attribute>
      <name>onLoad</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Javascript to run when the page is loaded</description>
    </attribute>
    <attribute>
      <name>onUnload</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Javascript to run when the page is unloaded</description>
    </attribute>
    <attribute>
      <name>onResize</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Javascript to run when the window is resized</description>
    </attribute>
    <attribute>
      <name>classStyle</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Class style to apply to form body</description>
    </attribute>
  </tag>

  <tag bb:public="true">
    <name>coursePage</name>
    <tag-class>blackboard.servlet.tags.CoursePageTag</tag-class>
    <body-content>JSP</body-content>
    <description>
      Creates a course page element. It may be used to re-arrange elements on a page from
      interface to interface. It makes the course available to sub-tags
    </description>
    <attribute>
      <name>courseId</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Id of the course object</description>
    </attribute>
    <attribute>
      <name>course</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <type>blackboard.data.course.Course</type>
      <description>
        Seems to be intended to store the actual course object. This attribute is not
        currently used in any pages, but might be in use in an external building block.
      </description>
    </attribute>
  </tag>

  <tag bb:public="true">
    <name>courseTitleBar</name>
    <tag-class>blackboard.servlet.tags.CourseTitlebarTag</tag-class>
    <body-content>JSP</body-content>
    <description>Creates a course title bar</description>
  </tag>

  <tag bb:public="true">
    <name>titleBar</name>
    <tag-class>blackboard.servlet.tags.TitlebarTag</tag-class>
    <body-content>JSP</body-content>
    <description>Creates a general use title bar.</description>
    <attribute>
      <name>iconUrl</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The URL for the Icon image used in the titlebar. Default is "/images/ci/icons/bookopen_u.gif".</description>
    </attribute>
    <attribute>
      <name>pluginId</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The Id for the plugin - known by the plugin developer and used as the directory
        root of the plugin. For instance:
        <![CDATA[<p>]]>
        "/Blackboard/plugins/*pluginId*/images/pluginimg.gif"
      </description>
    </attribute>
     <attribute>
      <name>isSequenceMapStyle</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Whether the titlebar is being used in a wizard or sequence map.
        Intended for internal use only.
      </description>
    </attribute>
  </tag>

  <tag bb:public="true">
    <name>titlebarButton</name>
    <tag-class>blackboard.servlet.tags.TitlebarButtonTag</tag-class>
    <body-content>empty</body-content>
    <description>
      A button that appears on the titlebar next to the page title.
    </description>
    <attribute>
      <name>title</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Title Bar button title</description>
    </attribute>
    <attribute>
      <name>href</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>URL where the link for the button points</description>
    </attribute>
    <attribute>
      <name>imgUrl</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The Image URL relative to the images directory (for example,
        "/images/ci/icons/add_item.gif") for the button as an icon.
      </description>
    </attribute>
    <attribute>
      <name>target</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        HTML link attribute to control which window the link is opened in
      </description>
    </attribute>
    <attribute>
      <name>folder</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Used for WebDAV folder attribute</description>
    </attribute>
  </tag>

  <tag bb:public="true">
    <name>titlebarContextButton</name>
    <tag-class>blackboard.servlet.tags.TitlebarContextButtonTag</tag-class>
    <body-content>JSP</body-content>
    <description>
      Container of multiple title bar buttons. Button whose title matches to active attribute is active while the rest are inactive.
      If no active attribute is provide, the first button is active by default.
    </description>
    <attribute>
      <name>active</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Specifies active button</description>
    </attribute>
    <attribute>
      <name>buttons</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>List of titlebarButtonBeans</description>
    </attribute>
  </tag>

  <tag bb:public="true">
    <name>instructionBar</name>
    <tag-class>blackboard.servlet.tags.InstructionbarTag</tag-class>
    <body-content>JSP</body-content>
    <description>
      Creates a general use instruction bar to explain how the page is to be used
    </description>
  </tag>

  <tag bb:public="true">
    <name>step</name>
    <tag-class>blackboard.servlet.tags.StepTag</tag-class>
    <body-content>JSP</body-content>
    <description>Creates html for a wizard Step element.</description>
    <attribute>
      <name>number</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The number for the step (1, 2, 3 for 1st, 2nd, 3rd in wizard)
      </description>
    </attribute>
    <attribute>
      <name>numberId</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        <![CDATA[<i>deprecated</i>]]>
        The number for the step (1, 2, 3 for 1st, 2nd, 3rd in wizard)
      </description>
    </attribute>
    <attribute>
      <name>numbered</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Whether this step should display the step number. Defaults to true.
      </description>
    </attribute>
    <attribute>
      <name>title</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The title of the step</description>
    </attribute>
    <attribute>
      <name>width</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        <![CDATA[<i>deprecated</i>]]>
        The width of the inner (content) table in the Step. Should not normally need to be
        set except in special instances such as when right-aligned content (such as a
        button)
        <![CDATA[<i>]]>
        must
        <![CDATA[</i>]]>
        align at the far right edge of the outer table. Can either be in the format of a
        pixel width ("250") or of a percent ("100%")
      </description>
    </attribute>
  </tag>

  <tag bb:public="true">
    <name>stepSubmit</name>
    <tag-class>blackboard.servlet.tags.StepSubmitTag</tag-class>
    <body-content>empty</body-content>
    <description>Creates html for ane entire submit/cancel step element</description>
    <attribute>
      <name>number</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The number for the step (1, 2, 3 for 1st, 2nd, 3rd in wizard)
      </description>
    </attribute>
    <attribute>
      <name>numberId</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        <![CDATA[<i>deprecated</i>]]>
        The number for the step (1, 2, 3 for 1st, 2nd, 3rd in wizard)
      </description>
    </attribute>
    <attribute>
      <name>title</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The title of the step</description>
    </attribute>
    <attribute>
      <name>cancelUrl</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The url to recall the page to if the user cancels</description>
    </attribute>
    <attribute>
      <name>target</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The target frame the page will be rendered</description>
    </attribute>
    <attribute>
      <name>previewUrl</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The url for previews, this is a onClick functionality</description>
    </attribute>
    <attribute>
      <name>instructions</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The message string displayed in this step</description>
    </attribute>
    <attribute>
      <name>submitButton</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        This optional key is used to look up the button and alt text for a nonstandard
        button, such as "Next". usage: submitButton="next". The user must make sure that
        there is a corresponding image and bundle string
        <![CDATA[<p>]]>
        image would be
        \builds\MAINLINE\base\content\locale\en_US\images\ci\formbtns\next_off.gif bundle
        entry would would be tags.properties: button.label.next=Next
      </description>
    </attribute>
  </tag>

  <tag bb:public="true">
    <name>breadcrumbBar</name>
    <tag-class>blackboard.servlet.tags.BreadcrumbBarTag</tag-class>
    <body-content>JSP</body-content>
    <description>Creates the framework for the breadcrumb navigation</description>
    <attribute>
      <name>environment</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Whether we are viewing this navigation in the course, control panel, portal, etc.
        Acceptable values are:
        <![CDATA[<ul>]]>
        <![CDATA[<li>]]>
        PORTAL
        <![CDATA[<li>]]>
        COURSE
        <![CDATA[<li>]]>
        CTRL_PANEL
        <![CDATA[<li>]]>
        SYS_ADMIN
      </description>
    </attribute>
    <attribute>
      <name>handle</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The database field used to uniquely identify the root nav item for the breadcrumb
        bar. (bar gets the appropriate nav item from the handler, then traverses the
        navigation tree to iteratively get the item's parents
      </description>
    </attribute>
    <attribute>
      <name>tabId</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The top-level origin tab ID. If set, in conjunction with the PORTAL environment,
        prepends a crumb for the parent tab to the crumb provided by getHandle
      </description>
    </attribute>
    <attribute>
      <name>isContent</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Indicates whether this navigation bar should render content components. If set,
        the tag will use context information to * display the "content" portion of the
        breadcrumb
      </description>
    </attribute>
    <attribute>
      <name>target</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The default target for bc items in this bar, defaults to none
      </description>
    </attribute>
    <attribute>
      <name>breadcrumbs</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <type>java.util.Collection</type>
      <description>You can also pass in a collection of Breadcrumb objects</description>
    </attribute>
    <attribute>
      <name>mode</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Enables the display view. Acceptable value is: ENABLE</description>
    </attribute>
    <attribute>
      <name>suppressCourse</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Suppresses the current course from the view. Default: false</description>
    </attribute>
  </tag>

  <tag bb:public="true">
    <name>breadcrumb</name>
    <tag-class>blackboard.servlet.tags.BreadcrumbTag</tag-class>
    <body-content>JSP</body-content>
    <description>Navigation item for the breadcrumb bar.  Body of this tag should contain the text to display for this breadcrumb</description>
    <attribute>
      <name>href</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The optional attribute setting the link for the element</description>
    </attribute>
    <attribute>
      <name>target</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The target attribute for the links in this breadcrumb, defaults to none
      </description>
    </attribute>
  </tag>

  <tag>
    <name>formBody</name>
    <tag-class>blackboard.servlet.form.FormBodyTag</tag-class>
    <body-content>JSP</body-content>
    <description>This tag represents the contents of an HTML form</description>
    <attribute>
      <name>name</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The name associated with the form.  Used internally by some of the more complicated elements.
      </description>
    </attribute>
    <attribute>
      <name>formBody</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The associated object depicting the structure of the form contents.
      </description>
    </attribute>
    <attribute>
      <name>formType</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Field controlling what type of view to apply to the form body.
      </description>
    </attribute>
    <attribute>
      <name>defaultAllowed</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Notifies rendering layer that default values are allowed or not.  True by default.
      </description>
    </attribute>
    <attribute>
      <name>hideStepLabel</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Notifies rendering layer step numbering should be hidden.
      </description>
    </attribute>
    <attribute>
      <name>showSelectionBoxes</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Notifies rendering layer that selection checkboxes should be added.
      </description>
    </attribute>
  </tag>

  <tag>
    <name>gridControl</name>
    <tag-class>blackboard.servlet.tags.grid.GridControlTag</tag-class>
    <body-content>JSP</body-content>
    <description>This tag represents a grid view</description>
    <attribute>
      <name>gridView</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The associated object depicting the structure of the grid.
      </description>
    </attribute>
    <attribute>
      <name>columnLabel</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Field controlling label for Y-Axis
      </description>
    </attribute>
    <attribute>
      <name>rowLabel</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Field controlling label for X-Axis
      </description>
    </attribute>
    <attribute>
      <name>customScript</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Field specifying location of addtional javascript file.
      </description>
    </attribute>
    <attribute>
      <name>isReadOnly</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Flag denoting if read-only override should be applied.
      </description>
    </attribute>
     <attribute>
      <name>isSafeEditMode</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
